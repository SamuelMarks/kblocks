# Benchmark a pipeline
# Requires:
# problem
# pipeline
# split (defaults to 'train')

import kblocks.framework.benchmark
import kblocks.framework.pipelines

benchmark.op_fn = @kb.framework.pipeline_benchmark
pipeline_benchmark.problem = %problem
pipeline_benchmark.split = %split
pipeline_benchmark.pipeline = %pipeline

split = 'train'
main.fn = @kb.framework.benchmark
